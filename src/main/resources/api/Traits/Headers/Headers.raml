#%RAML 1.0 Trait
headers:
  correlationId:
    type: string
    required: true
    minLength: 1
    maxLength: 60
    example: "adfs-kfjla-lkfj"
  appName:
    type: string
    required: true
    minLength: 1
    maxLength: 20
    example: "hl-sys-income"
  client_id:
    type: string
    required: true
    minLength: 1
    maxLength: 35
  client_secret:
    type: string
    required: true
    minLength: 1
    maxLength: 35

  
